{
    "manifest_version": 3,
    "name": "Bookmark New Tab",
    "version": "1.0",
    "description": "Custom new tab page with bookmarks",
    "icons": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png",
        "192": "icons/android-chrome-192x192.png",
        "512": "icons/android-chrome-512x512.png"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "dist/new-tab.html"
    }
}